{
  "name": "Personal-Website-V2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run scaffold && cross-env NODE_ENV=production webpack -p --progress && bundle exec jekyll build --source src/site --destination serve",
    "build:debug": "npm run scaffold && webpack --progress -d --verbose && bundle exec jekyll build -t",
    "start": "npm run scaffold && concurrently -k -n \"WEBPACK,JEKYLL,BROWSER-SYNC\" -c \"yellow.bold,magenta.bold,blue.bold\" \"webpack -w\" \"npm run jekyll\" \"npm run browsersync \"",
    "watch": "webpack -w",
    "jekyll": "bundle exec jekyll build --watch --limit_posts 1 --lsi 1 --incremental --drafts --source src/site --destination serve",
    "lint": "eslint",
    "lint:staged": "lint-staged",
    "test": "jest",
    "clean": "shx rm -rf serve/",
    "scaffold": "npm run clean && shx mkdir -p serve/js && shx mkdir -p serve/stylesheets",
    "browsersync": "wait-on -l ./serve/index.html && browser-sync --port 4000 start -s serve -f serve --extensions html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jrschellenberg/Personal-Website-V2.git"
  },
  "keywords": [],
  "author": "Justin Schellenberg",
  "bugs": {
    "url": "https://github.com/Jrschellenberg/Personal-Website-V2/issues"
  },
  "homepage": "https://github.com/Jrschellenberg/Personal-Website-V2#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/jest": "^24.0.15",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-lodash": "^3.3.4",
    "browser-sync": "^2.18.13",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "exports-loader": "^0.6.4",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "imports-loader": "^0.7.1",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.2",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/runtime-corejs2": "^7.1.5",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "dotenv-webpack": "^1.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.15.0",
    "smooth-scroll": "^16.1.0",
    "typed.js": "^2.0.11"
  },
  "proxy": "http://localhost:9000"
}
